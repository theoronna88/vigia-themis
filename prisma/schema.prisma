generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Student {
  id              String   @id @default(uuid())
  name            String
  father          String
  mother          String
  birthday        DateTime
  rg              String
  cpf             String
  phone           String
  tituloEleitoral String
  email           String
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
